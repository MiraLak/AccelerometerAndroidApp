<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="duchess.fr.basicaccelerometer">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.miralak.basicaccelerometer.activity.StartActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.miralak.basicaccelerometer.activity.AccelerometerActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.miralak.basicaccelerometer.activity.CollectDataActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.miralak.basicaccelerometer.activity.ConfigurationActivity"
            android:label="@string/app_name"></activity>
    </application>

</manifest>